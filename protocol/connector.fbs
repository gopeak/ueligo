// connector.fbs
namespace connector;


enum data_type : short
{  
    REQUEST    = 1,  
    PUSH  = 2,  
    BROTCAST   = 3  
}  
enum keeplive_type : short  
{  
    NO  = 0 ,
	YES = 1   
}  
table connector_data { 
	type:data_type=REQUEST;
	data:string;
	reqid:int;
	keeplive:keeplive_type=YES;
}

root_type connector_data;